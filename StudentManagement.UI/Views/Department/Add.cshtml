@model StudentManagement.UI.Models.AddDepartmentViewModel;


<h1 class="mt-3 mb-3"> Add Departrment</h1>

<form method="post">

    <div class="mt-3">
        <label class="form-label">Department Name</label>
        <input type="text" class="form-control" asp-for="DepartmentName" />
    </div>

    <div class="mt-3">
        <label class="form-label">Head</label>
        <input type="text" class="form-control" asp-for="Head" />

    </div>


    <div class="mt-3">
        <button class="btn btn-success" type="submit"> Save</button>
    </div>
</form>
